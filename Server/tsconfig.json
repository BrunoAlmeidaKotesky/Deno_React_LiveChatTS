{
    "compilerOptions": {
        "module": "esnext",
        "target": "es2015",
        "lib": [
          "es2015",
          "esnext.asynciterable"
        ],
        "jsx": "preserve",
        "sourceMap": true,
        "plugins": [
            {
              "name": "typescript-deno-plugin",
              "enable": true, 
              "importmap": "import_map.json"
            }
          ]
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ]
}